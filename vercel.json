{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/en/index.html",
      "destination": "/en/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "cruzdinishair.com" }
      ],
      "destination": "https://www.cruzdinishair.com/:path*",
      "permanent": true
    }
  ]
}
